<!DOCTYPE html>

<!--[if IEMobile 7 ]><html class="no-js iem7"><![endif]-->
<!--[if lt IE 7 ]><html class="no-js ie6" lang="en"><![endif]-->
<!--[if (IE 7)&!(IEMobile) ]><html class="no-js ie7" lang="en"><![endif]-->
<!--[if (IE 8)&!(IEMobile) ]><html class="no-js ie8" lang="en"><![endif]-->
<!--[if (gte IE 9)|(gt IEMobile 7)|!(IE)]><!--><html class="no-js" lang="en"><!--<![endif]-->

<head>
<meta charset="utf-8">

<title>Impact Dialing</title>
<meta name="description" content="">
<meta name="author" content="">

<!-- http://t.co/dKP3o1e -->
<meta name="HandheldFriendly" content="True">
<meta name="MobileOptimized" content="320">
<meta name="viewport" content="width=device-width, target-densitydpi=160dpi, initial-scale=1">
<%= javascript_include_tag :defaults %>
<%= unobtrusive_datepicker_includes %>

<!-- For all browsers -->
<link rel="stylesheet" media="screen" href="/css/style.css?v=1">
<link rel="stylesheet" media="print" href="/css/print.css?v=1">
<!-- For progressively larger displays -->
<link rel="stylesheet" media="only screen and (min-width: 480px)" href="/css/480.css?v=1">
<link rel="stylesheet" media="only screen and (min-width: 768px)" href="/css/768.css?v=1">
<link rel="stylesheet" media="only screen and (min-width: 992px)" href="/css/992.css?v=1">
<link rel="stylesheet" media="only screen and (min-width: 1382px)" href="/css/1382.css?v=1">
<!-- For Retina displays -->
<link rel="stylesheet" media="only screen and (-webkit-min-device-pixel-ratio: 1.5), only screen and (-o-min-device-pixel-ratio: 3/2), only screen and (min-device-pixel-ratio: 1.5)" href="/css/2x.css?v=1">

<!--[if (lt IE 9) & (!IEMobile)]>
<link rel="stylesheet" media="screen" href="/css/480.css?v=1">
<link rel="stylesheet" media="screen" href="/css/768.css?v=1">
<link rel="stylesheet" media="screen" href="/css/992.css?v=1">
<![endif]-->
<%= yield :stylesheets %>

<!-- JavaScript at bottom except for Modernizr -->
<script src="/js/libs/modernizr-custom.js"></script>

<!-- For iPhone 4 -->
<link rel="apple-touch-icon-precomposed" sizes="114x114" href="/img/h/apple-touch-icon.png">
<!-- For iPad 1-->
<link rel="apple-touch-icon-precomposed" sizes="72x72" href="/img/m/apple-touch-icon.png">
<!-- For iPhone 3G, iPod Touch and Android -->
<link rel="apple-touch-icon-precomposed" href="/img/l/apple-touch-icon-precomposed.png">
<!-- For Nokia -->
<link rel="shortcut icon" href="/img/l/apple-touch-icon.png">
<!-- For everything else -->
<link rel="shortcut icon" href="/favicon.ico">

<!--iOS. Delete if not required -->
<meta name="apple-mobile-web-app-capable" content="yes">
<meta name="apple-mobile-web-app-status-bar-style" content="black-translucent">
<link rel="apple-touch-startup-image" href="/img/splash.png">

<!--Microsoft. Delete if not required -->
<meta http-equiv="cleartype" content="on">
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

<!-- http://t.co/y1jPVnT -->
<link rel="canonical" href="/">
<%= stylesheet_link_tag "application" %>
</head>

<body>
<!-- Header -->
<div id="header">
  <div class="shell">
    <!-- Logo + Top Nav -->
    <div id="top">
      <h1><a href="/client">Impact Dialing</a></h1>
      <div id="top-navigation"> 

	<% if !session[:caller].blank? && @caller!=nil %>
	<ul class="clearfix">
	    <li class='divider'><a href="/caller" class='divider'>Home</a></li>
	    <li><a href="/caller/logout">Log out</a></li>
	  </ul>
	<% end %> 

	</div>

    </div>
    <!-- End Logo + Top Nav -->
    <!-- Main Nav -->
    <div id="navigation">
	<!--
      <ul>
        <li><a href="/client" <% if @actionName=="index" %>class="active"<% end %>><span>Welcome</span></a></li>
        <li><a href="/client/callers" <% if @actionName.index("caller") %>class="active"<% end %>><span>Callers</span></a></li>
        <li><a href="/client/scripts" <% if @actionName.index("script") %>class="active"<% end %>><span>Scripts</span></a></li>
        <li><a href="/client/campaigns" <% if @actionName.index("upload") || @actionName.index("campaign") %>class="active"<% end %>><span>Campaigns</span></a></li>
        <li><a href="/client/reports" <% if @actionName.index("report") %>class="active"<% end %>><span>Reports</span></a></li>
      </ul>
-->
    </div>
    <!-- End Main Nav -->
  </div>
</div>
<!-- End Header -->
<!-- Container -->
<div id="container">
  <div class="shell">
    <!-- Small Nav -->
    <div class="small-nav"> <a href="/caller">Welcome</a>
	<% if @breadcrumb.class.to_s=="String" %>
		<span>&gt;</span> <%= @breadcrumb %> 
	<% elsif @breadcrumb!=nil %>
		<% @breadcrumb.each do |b| %>
			<% if b.class.to_s=="Hash" %>
				<span>&gt;</span>
				<a href="<%= b[b.keys.first] %>"><%= b.keys.first %></a>
			<% else %>
				<span>&gt;</span> <%= b %> 
			<% end %>
		<% end %>
	<% end %>
	</div>
    <!-- End Small Nav -->

    <%= render :partial => "shared/flash_messages" %>

    <br />
    <!-- Main -->
    <div id="main" <% if @actionName=="campaign_view" %>style="background-color:white; -moz-border-radius: 20px;  	    -webkit-border-radius: 20px;  padding:14px;	border:1px solid #b0722f;<% end %>   "> 
      <div class="cl">&nbsp;</div>
      <!-- Content -->
      <div id="content">
		<%= yield %>
      </div>
      <!-- End Content -->
      <!-- Sidebar -->
      <div id="sidebar" <% if @actionName=="campaign_view" %>style="width:190px; margin-top:2px; margin-right:6px;"<% end %>>
	 <%= yield :sidebar %>
      </div>
      <!-- End Sidebar -->
      <div class="cl">&nbsp;</div>
<% if @actionName=="campaign_view" %>
	<div class="buttons2" align="center">
		<input type="button" class="button" value="Save Campaign" onclick="$('callerForm').submit();">
    </div><br/>
<% end %>
    <!-- Main -->
  </div>
   <%= render :partial => "shared/flash_messages", :locals => {:display => [:warning]} %>
</div>

<!-- End Container -->
<!-- Footer -->
<div id="footer">
  <div class="shell"> <span class="left">&copy; 2010 Impact Dialing
	<span style="margin-left:20px;"><a href="http://www.impactdialing.com">Contact Us</a></span>
	</span>
	</div>
</div>
<%= @floatSidebar %>
<!-- End Footer -->
</body>
</html>
