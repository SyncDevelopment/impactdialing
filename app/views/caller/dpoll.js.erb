<% begin %>
	<% if (@on_call==nil || @on_call.on_call==false) %>
		window.location.href='/caller/campaign/<%=@on_call.campaign_id%>';
	<% else %>
		function rc(k,v){
			var url = '/caller/reconnect_rt?key=' + sk + '&k=' + k + '&v=' + v;
			$.ajax({
				type: 'GET',
				dataType: 'script',
				url: url
			});
		}
		<% render :partial => 'get_reconnect_key_value' %>
		<% if @k=="voter_start" %>
			if (currAttempt!='<%= @attempt.id %>')
				rc('<%= @k %>','<%= escape_javascript @v %>');
		<% else %>
			if (currAttempt!='waiting')
				rc('<%= @k %>','<%=  @v %>');
		<% end %>
		clearTimeout(t);
	<% end %>
<% rescue %>
<% end %>
