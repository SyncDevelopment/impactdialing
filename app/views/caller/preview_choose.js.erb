var voters=[];
var formatted_phone_numbers = [];
<% @voters.each do |v| %>
    formatted_phone_numbers.push( <%=number_to_phone(v.Phone.to_s, :groupings => [4, 3, 3], :delimiter => "-", :area_code => true).to_json%> );
	v=["<%= escape_javascript v.FirstName %>","<%= escape_javascript v.LastName %>","<%= escape_javascript v.Phone %>","<%=  v.id %>"]
	voters.push(v)
<% end %>
showPreview();
