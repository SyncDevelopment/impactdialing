<fieldset class="nested-fields question">
  <legend class="question_label">Question 1</legend>
    <%= f.hidden_field :id, {"class"=> "identity"} %>  

  <p>
    <label>Question:</label>
    <%= f.text_field :text %>
    <%= f.hidden_field :order %>
  </p>
  
  <%= render "script_possible_response", {f: f} %>  
  
</fieldset>
