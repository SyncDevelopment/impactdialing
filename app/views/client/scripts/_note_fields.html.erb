<fieldset class="nested-fields script_element orderable_element js-script-note">
  <div class="ui-icon ui-icon-arrowthick-2-n-s" style="top: -42px;left: -14px;position: relative;"></div>
  <legend class="text_field_label">Note</legend>
  <%= f.text_field :note %>
  <%= f.hidden_field :script_order  , {class: "script_element"} %>
  <p class="actions">
    <%= link_to_remove_association "Delete this note", f , {class: "action secondary note_delete"}%>
  </p>
</fieldset>
