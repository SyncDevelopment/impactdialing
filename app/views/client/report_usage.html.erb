<table>
	<tr>
		<td>Utilization (1-second increments)</td>
		<td>Minutes</td>
<!--		<td>Minutes</td> -->
	</tr>
	<tr>
		<td>Callers</td>
		<td><% @caller_util.each do |c| %> <%= c[0].to_i/60 %> <% end %> <% @caller_util.data_seek(0) %> </td>
<!--		<td><% @caller_util.each do |c| %> <%= (c[0].to_f/60).round %> <% end %> <% @caller_util.data_seek(0) %> </td> -->
	</tr>

		<tr>
			<td>Calls</td>
			<td><% @calls_util.each do |c| %> <%= c[0].to_i/60 %> <% end %> <% @calls_util.data_seek(0) %> </td>
	<!--		<td><% @caller_util.each do |c| %> <%= (c[0].to_f/60).round %> <% end %> <% @caller_util.data_seek(0) %> </td> -->
		</tr>


	<tr>
		<td colspan="2">&nbsp;</td>
	</tr>
	<tr>
		<td>Billable time (1-minute increments)</td>
		<td>Minutes</td>
	</tr>
	<tr>
		<td>Callers</td>
		<td><% @caller_util.each do |c| %> <%= c[1].to_i %> <% end %> <% @caller_util.data_seek(0) %> </td>
	</tr>
	<tr>
		<td>Calls</td>
		<td><% @calls_bill.each do |c| %> <%= c[1].to_i %> <% end %> <% @calls_bill.data_seek(0) %> </td>
	</tr>
	<tr>
		<td>Voicemails</td>
		<td><% @vm_bill.each do |c| %> <%= c[1].to_i %> <% end %> <% @vm_bill.data_seek(0) %> </td>
	</tr>
	<tr>
		<td>Abandoned Calls</td>
		<td><% @abandon_bill.each do |c| %> <%= c[1].to_i %> <% end %> <% @abandon_bill.data_seek(0) %> </td>
	</tr>
</table>
